<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pt.novaleaf.www.maisverde.AcercaActivity">

    <include layout="@layout/app_bar_settings"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"/>

    <RelativeLayout
        android:id="@+id/rLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/elQuatro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/elTres"
            android:layout_below="@+id/elTres"
            android:layout_marginTop="24dp"
            android:text="Tiago Sousa"/>

        <TextView
            android:id="@+id/elTres"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/elDois"
            android:layout_below="@+id/elDois"
            android:layout_marginTop="31dp"
            android:text="Vicente Almeida"/>

        <TextView
            android:id="@+id/elDois"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/elUm"
            android:layout_marginStart="53dp"
            android:layout_marginTop="29dp"
            android:text="Rúben Silva"/>

        <TextView
            android:id="@+id/elUm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/elDois"
            android:layout_below="@+id/nomeEmpresa"
            android:layout_marginTop="68dp"
            android:text="Hugo Mochão"/>

        <TextView
            android:id="@+id/elCinco"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/elQuatro"
            android:layout_below="@+id/elQuatro"
            android:layout_marginTop="29dp"
            android:text="André Milho"/>

        <TextView
            android:id="@+id/nomeEmpresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="97dp"
            android:text="NOVA Leaf"
            android:textColor="@color/colorGreen"
            android:textSize="50sp"/>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="250dp"
            android:layout_toEndOf="@+id/elTres"
            android:text="texto muito bonito acerca da nossa aplicaçao"
            android:textSize="20sp"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/elCinco"
            android:layout_marginStart="44dp"
            android:layout_marginTop="28dp"
            android:text="Por favor envie o seu feedback para:"/>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView2"
            android:layout_alignTop="@+id/textView2"
            android:layout_marginTop="23dp"
            android:text="novaleaf@gmail.com"/>
    </RelativeLayout>

</LinearLayout>
